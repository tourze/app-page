  "description": "一个简单的，基于页面的CMS，可以用于简单的新闻系统、博客系统",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "tourze",
      "email": "dev@tourze.com"
    }
  ],
  "require": {
    "tourze/base": "^1.0",
    "tourze/bootstrap": "^1.0",
    "tourze/controller": "^1.0",
    "tourze/view": "^1.0",
    "tourze/twig": "^1.0",
    "tourze/server": "^1.0"
  },
  "autoload": {
    "psr-4": {
